<div class="Category new">
<%=link_to("Back To Category",categories_path,:class=>"back-link")%>
<h1>Created Category</h1>
<%=form_for(@category,:method=>"post") do |f|%>
<%=render(:partial=>'form',:locals=>{:f=>f} )%>

<div class="forms-button">
<%=f.submit("Create Category")%>
</div>
<%end%>
</div>








